{
  "presets": [
    [
      "next/babel",
      {
        "styled-jsx": {
          "plugins": [
            [
              "@styled-jsx/plugin-sass",
              {
                "sassOptions": {
                  "includePaths": ["./styles"],
                  "prependData": "@import '~@styles/_variables.scss';"
                }
              }
            ]
          ]
        }
      }
    ]
  ]
}