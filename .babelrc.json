{
  "presets": [
    [
      "next/babel",
      {
        "styled-jsx": {
          "plugins": [
            [
              "@styled-jsx/plugin-sass",
              {
                "sassOptions": {
                  "includePaths": ["./styles"],
                  "data": "@import './styles/scss/_variables.module.scss';"
                }
              }
            ]
          ]
        }
      }
    ]
  ],
  "plugins": [["styled-components", { "ssr": true }]]
}